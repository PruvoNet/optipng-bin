{
	"name": "optipng-bin",
	"version": "9.1.0",
	"description": "OptiPNG wrapper that makes it seamlessly available as a local dependency",
	"license": "MIT",
	"repository": "imagemin/optipng-bin",
	"bin": {
		"optipng": "cli.js"
	},
	"engines": {
		"node": "^14.13.1 || >=16.0.0 || >= 18.0.0 || >= 20.0.0 || >= 22.0.0"
	},
	"scripts": {
		"postinstall": "node lib/install.js",
		"test": "xo && ava --timeout=120s"
	},
	"files": [
		"index.js",
		"cli.js",
		"lib",
		"vendor/source"
	],
	"keywords": [
		"imagemin",
		"compress",
		"image",
		"minify",
		"optimize",
		"png",
		"optipng"
	],
	"dependencies": {
		"bin-wrapper": "https://github.com/PruvoNet/bin-wrapper#777f45f627304f25df016929d32b0f803821d297",
		"bin-build": "^3.0.0"
	},
	"devDependencies": {
		"bin-check": "^4.0.1",
		"ava": "^4.2.0",
		"compare-size": "^3.0.0",
		"execa": "^1.0.0",
		"tempy": "^0.3.0",
		"xo": "^0.48.0"
	}
}
